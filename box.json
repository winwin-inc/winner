{
    "chmod": "0755",
    "compactors": [
        "Herrera\\Box\\Compactor\\Json",
        "Herrera\\Box\\Compactor\\Php"
    ],
    "directories": ["src"],
    "finder": [
        {
            "name": "*.php",
            "exclude": [
                "phpunit",
                "mikey179",
                "Tests",
                "friendsofphp",
                "phpunit-test-case",
                "Tester",
                "test",
                "tests",
                "phpspec"
            ],
            "in": "vendor"
        }
    ],
    "git-commit": "git-commit",
    "git-version": "git-version",
    "main": "bin/winner",
    "output": "winner.phar",
    "stub": true
}
